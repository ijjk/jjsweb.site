{
  "name": "jjsweb-site",
  "description": "My personal site",
  "version": "3.0.0",
  "author": "JJ Kasper <jj@jjsweb.site>",
  "keywords": [
    "Next.js",
    "react",
    "styled-jsx"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ijjk/jjsweb.site"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "lint": "next lint",
    "format": "prettier --write \"**/*.js\" --ignore-path .gitignore"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "eslint": "^7.28.0",
    "eslint-config-next": "^10.2.4-canary.15",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "next": "^11.0.0",
    "prettier": "^1.15.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
